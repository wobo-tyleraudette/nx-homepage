{
  "root": "libs/utilities-nonpub",
  "sourceRoot": "libs/utilities-nonpub/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/utilities-nonpub/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/utilities-nonpub"],
      "options": {
        "jestConfig": "libs/utilities-nonpub/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
